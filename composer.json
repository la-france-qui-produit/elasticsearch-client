{
    "name": "produits-de-france/elasticsearch-client",
    "type" : "symfony-bundle",
    "license": "proprietary",
    "require": {
        "php": "^7.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "elasticsearch/elasticsearch": "^7.6",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^3.4|^4.0|^5.0",
        "symfony/yaml": "^3.4|^4.0|^5.0"
    },
    "require-dev": {
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ProduitsDeFrance\\ElasticsearchClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProduitsDeFrance\\ElasticsearchClient\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    }
}
